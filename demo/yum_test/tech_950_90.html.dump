<script type="text/javascript">
    function XBTop(){
        this.dom = null;
        this.sec = 1500;
    }
    XBTop.prototype = {
        addEvent : function(ele,etype,func){
            if(document.addEventListener){ele.addEventListener(etype,func,false);}
            else if(document.attachEvent){ele.attachEvent('on' + etype,func);}
            else{ele['on' + etype] = function(){func();}}
        },
        isIE6 : (navigator.userAgent.toLowerCase().indexOf('msie') > -1) && (navigator.userAgent.toLowerCase().substr(navigator.userAgent.toLowerCase().indexOf('msie')).match(/\d+/)[0] == 6),
        getScrollSize : function(){return{
            top : Math.max(document.body.scrollTop,document.documentElement.scrollTop),
            left : Math.max(document.body.scrollLeft,document.documentElement.scrollLeft)}
        },
        toTop : function(){
            var that = this;
            var s = that.getScrollSize().top,
            sec = that.sec || 1500,
            interval,
            delay = 50,
            v = -(Math.max(delay*s/sec,100));
            interval = setInterval(function(){
                window.scrollBy(0,v);
                s = that.getScrollSize().top;
                if(s <= 0){clearInterval(interval);}
            },delay);
        },
        setVisible : function(){
            var that = this;
            var s = that.getScrollSize().top;
            if(s > that.vheight){that.dom.style.display='block';}
            else{that.dom.style.display='none';}},
        fixIE6Pos : function(){
            var that = this;
            if(that.top){
                var ph = that.getScrollSize().top + that.top;
                that.dom.style.position='absolute';
                that.dom.style.top = ph + 'px';
            }else if(that.bottom){
                var wh = Math.min(document.documentElement.clientHeight,document.body.clientHeight),
                    ph = wh + that.getScrollSize().top - that.dom.offsetHeight - that.bottom;
                that.dom.style.position='absolute';
                that.dom.style.top = ph + 'px';
            }
        },
        init : function(wid,btn,sec,vheight,top,bottom){
            var that = this;
            that.dom = document.getElementById(wid) || null;
            that.btn = document.getElementById(btn) || null;
            that.sec = sec;
            that.vheight = vheight;
            that.top = top;
            that.bottom = bottom;
            if(that.dom){
                that.addEvent(window,'load',function(){that.setVisible();});
                that.addEvent(window,'scroll',function(){that.setVisible();});
                that.addEvent(window,'resize',function(){that.setVisible();});
                that.addEvent(window,'load',function(){that.fixIE6Pos();});
                that.addEvent(window,'scroll',function(){that.fixIE6Pos();});
                that.addEvent(window,'resize',function(){that.fixIE6Pos();});
                if(that.btn){that.addEvent(that.btn,'click',function(){that.toTop();});}
            }
        }
    };
</script>
<script type="text/javascript">
if (!sina) var sina = {};
sina.addEvent = function(o, type, fn) {
    o.attachEvent ? o.attachEvent('on' + type, fn) : o.addEventListener(type, fn, false)
};
sina.delEvent = function(o, type, fn) {
    o.detachEvent ? o.detachEvent('on' + type, fn) : o.removeEventListener(type, fn, false)
};
sina.$ = function(o) {
    return document.getElementById(o)
};

function sim_select(o) {
    o = document.getElementById(o);//select
    o.style.display = 'none';
    var opts = o.options,
    parent = o.parentNode,
    self = this;
    this.isShow = false;
    this.div = document.createElement('div');
    this.ul = document.createElement('ul');
    this.h3 = document.createElement('h3');
    this.div.className = 'sim_select';
    parent.replaceChild(this.div, o);
    this.div.appendChild(o);
    this.ul.style.display = 'none';
    this.ul.style.top = this.h3.offsetHeight;
    this.h3.innerHTML = opts[o.selectedIndex].innerHTML;
    for (var i = 0, l = o.length; i < l; i++) {
        var li = document.createElement('li');
        li.innerHTML = opts[i].innerHTML;
        this.ul.appendChild(li);
        li.onmouseover = function() {
            this.className += ' over'
        };
        li.onmouseout = function() {
            this.className = this.className.replace(/over/gi, '')
        };
        li.onclick = (function(i) {
            return function() {
                self.hide();
                self.h3.innerHTML = this.innerHTML;
                o.selectedIndex = i;
                if (o.onchange) {
                    o.onchange()
                }
            }
        })(i)
    };
    this.div.appendChild(this.h3);
    this.div.appendChild(this.ul);
    this.ul.style.top = this.h3.offsetHeight + 'px';
    this.ul.style.width = this.h3.offsetWidth - 2 + 'px';
    this.init()
};
sim_select.prototype = {
    init: function() {
        var self = this;
        sina.addEvent(document.documentElement, 'click', 
        function(e) {
            self.close(e)
        });
        this.h3.onclick = function() {
            self.toggles()
        }
    },
    show: function() {
        this.ul.style.display = 'block';
        this.isShow = true
    },
    hide: function() {
        this.ul.style.display = 'none';
        this.isShow = false
    },
    close: function(e) {
        var t = window.event ? window.event.srcElement: e.target;
        do {
            if (t == this.div) {
                return
            } else if (t == document.documentElement) {
                this.hide();
                return
            } else {
                t = t.parentNode
            }
        }
        while (t.parentNode)
    },
    toggles: function() {
        this.isShow ? this.hide() : this.show()
    }
}

</script>
